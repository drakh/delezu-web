  "dependencies": {
    "@material-ui/core": "^3.7.0",
    "@material-ui/icons": "^3.0.1",
    "@types/mongoose": "^5.0.17",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.42",
    "@types/http-proxy-middleware": "^0.19.1",
    "@types/jss": "^9.5.7",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-jss": "^8.6.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/recharts": "^1.1.5",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.21",
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "http-proxy-middleware": "^0.19.1",
    "jss": "^9.8.7",
    "nodemon": "^1.18.9",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-jss": "^8.6.1",
    "react-router-dom": "^4.3.1",
    "recharts": "^1.4.1",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "express-subdomain-middleware": "^1.0.11",
    "mongoose": "^5.1.6"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "concurrently \"npm run ts:watch\" \"npm run webpack:watch\" \"npm run dev:server\"",
    "dev:server": "cross-env APP_ENV=dev NODE_PATH=dist nodemon --inspect=1338 -w dist -d 1 dist/server/index.js",
    "build": "npm run ts && npm run webpack",
    "ts": "tsc --pretty",
    "ts:watch": "tsc --pretty --watch",
    "webpack": "webpack --mode=\"production\" --cache --progress --colors --config dist/webpack/config.js",
    "webpack:watch": "webpack --mode=\"development\" --watch --cache --progress --colors --config dist/webpack/config.js",
    "tslint": "tslint -c ./tslint.json --project tsconfig.json \"src/**/*.ts*\"",
    "start": "npm run build && npm run dev",
    "prod": "cross-env NODE_PATH=dist node dist/server/index.js"
  },
